{
  "name": "vega-neutrino",
  "version": "0.1.0",
  "description": "Neutrino integration for Vega large dataset support with columnar storage.",
  "keywords": [
    "vega",
    "neutrino",
    "columnar",
    "wasm",
    "performance",
    "large-data",
    "aggregation"
  ],
  "license": "BSD-3-Clause",
  "type": "module",
  "author": {
    "name": "Vega",
    "url": "https://vega.github.io"
  },
  "main": "build/vega-neutrino.js",
  "module": "build/vega-neutrino.module.js",
  "unpkg": "build/vega-neutrino.min.js",
  "exports": {
    "default": "./build/vega-neutrino.js"
  },
  "files": [
    "src",
    "build",
    "wasm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vega/vega.git"
  },
  "scripts": {
    "prebuild": "del-cli build",
    "build": "rollup -c rollup.config.js",
    "pretest": "npm run build -- --config-test",
    "test": "tape 'test/**/*-test.js'",
    "prepublishOnly": "npm test && npm run build"
  },
  "dependencies": {
    "vega-dataflow": "^6.1.0",
    "vega-loader": "^5.1.0",
    "vega-util": "^2.1.0"
  },
  "peerDependencies": {
    "vega": "^5.0.0"
  }
}
